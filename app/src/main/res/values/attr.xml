<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="toolbarTitle" format="string"/>
    <attr name="mineIcon" format="reference"/>
    <attr name="mineTitle" format="string"/>
    <attr name="tabVisibleCount" format="integer"/><!-- tab的可见的数量-->
    <attr name="CircleImageView_border_width" format="dimension"/>
    <attr name="CircleImageView_border_color" format="reference"/>
    <attr name="userInfoViewTitle" format="string"/>
    <attr name="userInfoViewContent" format="string"/>


    <declare-styleable name="CustomToolbar">
        <attr name="toolbarTitle"/><!-- toolbar的标题-->
    </declare-styleable>


    <declare-styleable name="MineLayout">
        <attr name="mineIcon"/>
        <attr name="mineTitle"/>
    </declare-styleable>
    <declare-styleable name="ViewPagerIndicator">
        <attr name="tabVisibleCount"/>
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="CircleImageView_border_width" />
        <attr name="CircleImageView_border_color" />
    </declare-styleable>


    <declare-styleable name="UserInfoView">
        <attr name="userInfoViewTitle" />
        <attr name="userInfoViewContent" />
    </declare-styleable>

</resources>